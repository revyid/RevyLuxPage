{
  "redirects": [
    {
      "source": "/404",
      "destination": "/404.html",
      "permanent": false
    },
    {
      "source": "/index",
      "destination": "/login.html",
      "permanent": true
    },
    {
      "source": "/index.html",
      "destination": "/login.html",
      "permanent": true
    }
  ],
  "rewrites": [
    {
      "source": "/:path*",
      "destination": "/:path*.html"
    },
    {
      "source": "/",
      "destination": "/login.html"
    }
  ],
  "headers": [
    {
      "source": "/:path*",
      "headers": [
        {
          "key": "Content-Security-Policy",
          "value": "default-src 'self';"
        }
      ]
    }
  ]
}
