{
  "redirects": [
    {
      "source": "/404",
      "destination": "/404.html",
      "permanent": false
    },
    {
      "source": "/home",
      "destination": "/index.html",
      "permanent": true
    }
  ],
  "rewrites": [
    {
      "source": "/:path*",
      "destination": "/:path*.html"
    },
    {
      "source": "/",
      "destination": "/index.html"
    }
  ]
}
